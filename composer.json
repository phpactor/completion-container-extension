{
    "name": "phpactor/completion-container-extension",
    "description": "CompletionContainer",
    "license": "MIT",
    "minimum-stability": "dev",
    "type": "phpactor-extension",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "phpactor/container": "^1.0",
        "phpactor/completion-extension": "~0.1",
        "phpactor/worse-reflection-extension": "~0.1",
        "phpactor/source-code-filesystem-extension": "~0.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5@stable",
        "phpunit/phpunit": "^7.4@stable",
        "friendsofphp/php-cs-fixer": "^2.13@stable",
        "phpactor/test-utils": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\CompletionContainer\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\CompletionContainer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Extension\\CompletionContainer\\CompletionContainerExtension",
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
